<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= product.type %>
  </p>

  <p>
    <strong>Length:</strong>
    <%= product.length %>
  </p>

  <p>
    <strong>Width:</strong>
    <%= product.width %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= product.height %>
  </p>

  <p>
    <strong>Weight:</strong>
    <%= product.weight %>
  </p>

</div>
